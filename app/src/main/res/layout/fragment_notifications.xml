<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E7F4F9"
    tools:context=".ui.notifications.NotificationsFragment">

    <TextView
        android:id="@+id/member"
        android:layout_width="137dp"
        android:layout_height="35dp"
        android:layout_marginTop="48dp"
        android:text="組員名單："
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <TextView
        android:id="@+id/txt11056006"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="11056006林郁軒"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/member"
        app:layout_constraintStart_toStartOf="@+id/member"
        app:layout_constraintTop_toBottomOf="@+id/member" />

    <TextView
        android:id="@+id/txt11056049"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11056049鄭希妍"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/txt11056022"
        app:layout_constraintTop_toBottomOf="@+id/txt11056022" />

    <TextView
        android:id="@+id/txt11056022"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11056022陳思妤"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/txt11056006"
        app:layout_constraintTop_toBottomOf="@+id/txt11056006" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="72dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/laptop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.056" />

</androidx.constraintlayout.widget.ConstraintLayout>